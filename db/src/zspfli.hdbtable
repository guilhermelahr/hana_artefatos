COLUMN TABLE zspfli (
	"MANDT" NVARCHAR(3),
	"CARRID" NVARCHAR(3),
	"CONNID" NVARCHAR(4),
	"COUNTRYFR" NVARCHAR(3),
	"CITYFROM" NVARCHAR(20),
	"AIRPFROM" NVARCHAR(3),
	"COUNTRYTO" NVARCHAR(3),
	"CITYTO" NVARCHAR(20),
	"AIRPTO" NVARCHAR(3),
	"FLTIME" INTEGER,
	"DEPTIME" TIME,
	"ARRTIME" TIME,
	"DISTANCE" DECIMAL(9, 4),
	"DISTID" NVARCHAR(3),
	"FLTYPE" NVARCHAR(1),
	"PERIOD" TINYINT,
	"LASTUPDATE" LONGDATE,
  PRIMARY KEY(mandt, carrid, connid)
)